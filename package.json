{
  "name": "@fresh8/prom-client-timer-unit",
  "version": "1.0.0",
  "description": "A helper function to help record time in other units than seconds.",
  "main": "index.js",
  "dependencies": {},
  "devDependencies": {
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "husky": "^0.14.3",
    "lint-staged": "^6.1.0",
    "mocha": "^5.0.0",
    "nyc": "^11.4.1",
    "prettier": "^1.10.2"
  },
  "scripts": {
    "coverage": "nyc npm test && nyc report --reporter=lcov",
    "coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "precommit": "lint-staged",
    "test": "mocha ./test",
    "test-watch": "mocha ./test --watch"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write --single-quote --no-semi",
      "git add"
    ]
  },
  "nyc": {
    "watermarks": {
      "lines": [
        80,
        95
      ],
      "functions": [
        80,
        95
      ],
      "branches": [
        80,
        95
      ],
      "statements": [
        80,
        95
      ]
    },
    "extension": [],
    "exclude": [
      "**/*.d.ts"
    ],
    "require": [],
    "reporter": [
      "html",
      "text-summary",
      "json"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fresh8/prom-client-timer-unit.git"
  },
  "author": "Fresh8",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fresh8/prom-client-timer-unit/issues"
  },
  "homepage": "https://github.com/fresh8/prom-client-timer-unit#readme",
  "engines": {
    "node": ">=6.9.0",
    "npm": ">=5"
  },
  "typings": "index.d.ts"
}
